{
    "plugins": ["styled-components-a11y"],
    "extends": [
      "./node_modules/@dsch/eslint-rules",
      "plugin:styled-components-a11y/recommended"
    ],
    "rules": {
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/label-has-for": 0,
      "styled-components-a11y/anchor-is-valid": 0,
      "styled-components-a11y/click-events-have-key-events": 0,
      "styled-components-a11y/no-static-element-interactions": 1,
      "styled-components-a11y/role-supports-aria-props": 0,
      "styled-components-a11y/mouse-events-have-key-events": 1,
      "styled-components-a11y/no-noninteractive-tabindex": 1,
      "styled-components-a11y/no-noninteractive-element-interactions": 1,
      "jsx-a11y/mouse-events-have-key-events": 1,
      "styled-components-a11y/label-has-associated-control": 1,
      "styled-components-a11y/interactive-supports-focus": 1,
      "styled-components-a11y/scope": 1
    }
  }
  